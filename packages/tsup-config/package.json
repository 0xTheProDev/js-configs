{
  "name": "@theprodev/tsup-config",
  "version": "0.1.0",
  "description": "A Decorated and Opinionated Configuration for Tsup Builder",
  "keywords": [
    "nodejs",
    "web",
    "microservices",
    "typescript",
    "angular",
    "nestjs",
    "nextjs",
    "react",
    "vue"
  ],
  "bugs": {
    "url": "https://github.com/0xTheProDev/js-configs/discussions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xTheProDev/js-configs.git"
  },
  "license": "MIT",
  "author": "Progyan Bhattacharya <bprogyan@gmail.com>",
  "type": "module",
  "main": "dist/tsup.config.js",
  "module": "dist/tsup.config.mjs",
  "types": "dist/tsup.config.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier \"./**/*.{js,ts,tsx,json,md}\" --check",
    "format:fix": "prettier --write \"./**/*.{js,ts,tsx,json,md}\"",
    "lint": "eslint \"./src/**/*.{cjs,mjs,js,ts,tsx,json}\"",
    "lint:fix": "eslint \"./src/**/*.{cjs,mjs,js,ts,tsx,json}\" --fix",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@theprodev/prettier-config",
  "devDependencies": {
    "@theprodev/eslint-config": "workspace:*",
    "@theprodev/tsconfigs": "workspace:*",
    "tsup": "8.2.4"
  },
  "peerDependencies": {
    "tsup": "^8"
  },
  "publishConfig": {
    "access": "public"
  }
}
